@page
@model EjemploProyecto.Pages.ADM_Identificacion.CreateModel

<h2>Crear Tipo de Identificación</h2>

<form method="post">
    <div class="mb-3">
        <label class="form-label">Nombre del Tipo</label>
        <input asp-for="Tipo.Tipo_Identificacion" class="form-control" />
        <span asp-validation-for="Tipo.Tipo_Identificacion" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-success">Guardar</button>
    <a asp-page="Index" class="btn btn-secondary">Volver</a>
</form>

