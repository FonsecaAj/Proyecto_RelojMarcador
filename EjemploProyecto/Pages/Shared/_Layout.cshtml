@{
    Layout = null;
    var title = ViewData["Title"] as string ?? "EjemploProyecto";
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@title - EjemploProyecto</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container">
                <a class="navbar-brand" asp-page="/Index">EjemploProyecto</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#topnav">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="topnav">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item"><a class="nav-link" asp-page="/Index">Home</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Privacy">Privacy</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Personas/Index">Personas</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Admin/Usuarios/Index">Usuarios</a></li>
                        <li class="nav-item"><a class="nav-link text-dark" asp-page="/Admin/Areas/Index">Áreas</a></li>

                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Personas/Index">Personas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/PRUEBA_Usuarios/Index">Usuarios</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="container py-4">
        <partial name="_Alerts" />
        @RenderBody()
    </div>

    <partial name="_Modal" />

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
